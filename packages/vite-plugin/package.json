{
  "name": "@cloudwerk/vite-plugin",
  "version": "0.5.0",
  "description": "Vite plugin for Cloudwerk file-based routing with virtual entry generation",
  "repository": {
    "type": "git",
    "url": "https://github.com/squirrelsoft-dev/cloudwerk.git",
    "directory": "packages/vite-plugin"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "test": "vitest --run --passWithNoTests",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@cloudwerk/core": "workspace:^",
    "@cloudwerk/ui": "workspace:^",
    "@swc/core": "^1.3.100"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0",
    "@hono/vite-dev-server": ">=0.18.0",
    "hono": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "@hono/vite-dev-server": {
      "optional": true
    }
  },
  "devDependencies": {
    "@hono/vite-dev-server": ">=0.18.0",
    "@types/node": "^20.0.0",
    "hono": "^4.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0",
    "vitest": "^1.0.0"
  }
}
