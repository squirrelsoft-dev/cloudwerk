{
  "name": "@cloudwerk/core",
  "version": "0.2.0",
  "description": "Routing, middleware, config for Cloudwerk",
  "repository": {
    "type": "git",
    "url": "https://github.com/squirrelsoft-dev/cloudwerk.git",
    "directory": "packages/core"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
