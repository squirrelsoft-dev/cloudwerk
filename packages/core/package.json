{
  "name": "@cloudwerk/core",
  "version": "0.7.0",
  "description": "Routing, middleware, config for Cloudwerk",
  "repository": {
    "type": "git",
    "url": "https://github.com/squirrelsoft-dev/cloudwerk.git",
    "directory": "packages/core"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./runtime": {
      "types": "./dist/runtime.d.ts",
      "import": "./dist/runtime.js"
    },
    "./build": {
      "types": "./dist/build.d.ts",
      "import": "./dist/build.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --run --coverage"
  },
  "dependencies": {
    "@cloudwerk/utils": "workspace:*",
    "esbuild": "^0.25.0",
    "fast-glob": "^3.3.0",
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "tsup": "^8.0.0",
    "@vitest/coverage-v8": "^1.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
